<h1 align="center">All Users</h1><hr>
<div align="center">
<% @users.each do |user| %>
 <ul class="listing">
  <div class="row">
   <div class="well col-md-4 col-md-offset-4">
    <li><%= gravatar_for(user)%></li>
    <li class="article-title"><%= user.username%></li>
    <li><small><%= pluralize(user.articles.count, "article") if user.articles %></small></li>
   </div>
  </div>
 </ul>
<% end %>
</div>
<center>
<%= will_paginate %>
</center>